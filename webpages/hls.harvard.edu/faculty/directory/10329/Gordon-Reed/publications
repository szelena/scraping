      <script type="text/javascript">/*
 * jQuery UI Autocomplete HTML Extension
 *
 * Copyright 2010, Scott González (http://scottgonzalez.com)
 * Dual licensed under the MIT or GPL Version 2 licenses.
 *
 * http://github.com/scottgonzalez/jquery-ui-extensions
 */
(function( $ ) {

var proto = $.ui.autocomplete.prototype,
	initSource = proto._initSource;

function filter( array, term ) {
	var matcher = new RegExp( $.ui.autocomplete.escapeRegex(term), "i" );
	return $.grep( array, function(value) {
		return matcher.test( $( "<div>" ).html( value.label || value.value || value ).text() );
	});
}

$.extend( proto, {
	_initSource: function() {
		if ( this.options.html && $.isArray(this.options.source) ) {
			this.source = function( request, response ) {
				response( filter( this.options.source, request.term ) );
			};
		} else {
			initSource.call( this );
		}
	},

	_renderItem: function( ul, item) {
		return $( "<li></li>" )
			.data( "item.autocomplete", item )
			.append( $( "<a></a>" )[ this.options.html ? "html" : "text" ]( item.label ) )
			.appendTo( ul );
	}
});

})( jQuery );
</script><style type="text/css">/* FacultyDetail.css */

img.headshot {
  border-bottom: 24px solid #4E84C4;
  display: block;
  float: left;
  height: 200px;
  margin: -5em 2em 2em 0;
}

h2 { font-weight: bold; }
h2.title { font-size: 130%; }

div.faculty-detail-info { margin-bottom: 1em; margin-top: 190px; }
p.faculty-info { margin: .3em 0 0; }
ul.links { margin: 1.5em 0 0; }
ul.links + ul.links { margin: 0; }

ul.tabs li a {
  background-color: #BAC5C6;
  color: #1E1E1E;
}
ul.tabs li a:hover {
  background-color: #D16103;
  color: #fff;
}
.active-panel h2 { margin-top: 2em; }

</style>
<h3>Annette Gordon-Reed</h3>
      <div >
        <div class="multi-column-wrapper">
                <div class="multi-column-three-quarters">
        <h2 class="title">Charles Warren Professor of American Legal History</h2><h2 class="title">Carol K. Pforzheimer Professor, Radcliffe Institute for Advanced Study</h2><h2 class="title">Professor of History, Faculty of Arts & Sciences</h2>
        <div class="panel-top-tabs pinned-container">
          <ul class="tabs"><li><a href="/faculty/directory/10329/Gordon-Reed/">About</a></li><li><a href="/faculty/directory/10329/Gordon-Reed/publications" class="selected">Publications</a></li></ul>
          <div class="active-panel"><p class="right-align"><a href="/faculty/directory/10329/Gordon-Reed/bibliography">Bibliography</a></p><h2>Representative Publications</h2><ul class="fac-section"><li>Gordon-Reed,  Annette. <i>The Hemingses of Monticello: An American Family</i> (W.W. Norton 2008).<br /></li><li><i>Race on Trial: Law and Justice in American History</i> ( Annette Gordon-Reed ed., Oxford University Press 2002).<br /></li><li>Jordan, Jr., Vernon E. &amp;  Annette Gordon-Reed. <i>Vernon Can Read: A Memoir</i> (Public Affairs 2001).<br /></li><li>Gordon-Reed,  Annette. <i>Thomas Jefferson and Sally Hemings: An American Controversy</i> (University of Virginia Press 1997).<br /></li></ul><p class="right-align"><a href="/faculty/directory/10329/Gordon-Reed/bibliography">Bibliography</a></p></div>
        </div>
      </div>
                <div class="multi-column-quarter">
